type timekeep, domain;
type timekeep_exec, exec_type, file_type;
type timekeep_service, service_manager_type;
type timekeep_data_file, file_type;
 
# Started by init
init_daemon_domain(timekeep)
 
#============= timekeep ==============
allow timekeep self:capability { sys_time dac_override fowner fsetid };
allow timekeep sysfs:file { open read };
allow timekeep time_data_file:file { getattr open setattr write };
allow timekeep timekeep_prop:file { getattr open read };

